(library (name css_parser) (public_name ppx_css.css_parser)
 (libraries menhirLib compiler-libs.common sedlex)
 (flags (:standard -w -27-34) -w -60) (preprocess (pps sedlex.ppx)))

(rule (targets menhir_parser.mli menhir_parser.ml)
 (deps (:first_dep menhir_parser.mly))
 (action (bash "%{bin:menhir} %{first_dep} 2>&1")))

(alias (name DEFAULT) (deps menhir_parser.ml))